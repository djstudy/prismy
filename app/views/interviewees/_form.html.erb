<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :name %>
  </div>
  <div class="col-sm-10">

    <%= f.text_field :name, autofocus: false, class: "form-control"%>
  </div>
</div>


<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :password %>
  </div>
  <div class="col-sm-10">
    <%= f.password_field :password, autofocus: false, class: "form-control"%>
  </div>
</div>



<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :password_confirmation %>
  </div>
  <div class="col-sm-10">
    <%= f.password_field :password_confirmation, autofocus: false, class: "form-control"%>
  </div>
</div>


<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :email %>
  </div>
  <div class="col-sm-10">

    <%= f.text_field :email, autofocus: true, class: "form-control"%>
  </div>
</div>




<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :description %>
  </div>
  <div class="col-sm-10">
    <%= f.text_field :description, autofocus: true, class: "form-control" %>
  </div>
</div>

<div class="form-group">
  <div class="col-sm-2">
    <%= f.label :profile_image %>
  </div>
  <div class="col-sm-10">
    <%= f.text_field :profile_image, autofocus: true, class: "form-control" %>
  </div>
</div>